{
  "name": "__MSG_extName__",
  "short_name": "__MSG_extShortName__",
  "description": "__MSG_extShortName__",
  "version": "2.0.0",
  "manifest_version": 3,
  "author": "Yuri Ellfort",
  "default_locale": "en",
  "action": {
    "default_icon": {
      "16": "images/icons/default-16.png",
      "48": "images/icons/default-48.png",
      "128": "images/icons/default-128.png"
    }
  },
  "permissions": [
    "tabs",
    "activeTab",
    "contextMenus",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "images/icons/default-16.png",
    "48": "images/icons/default-48.png",
    "128": "images/icons/default-128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
